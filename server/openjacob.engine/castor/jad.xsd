							<xs:element name="Dimension" type="CastorDimension"/>
							<xs:element name="Caption" type="CastorCaption" minOccurs="0"/>
							<!--  seit 2.8.3  -->
              <xs:element name="Font" type="CastorFont" minOccurs="0"/>
						</xs:sequence>
         		<xs:attribute name="halign" type="CastorHorizontalAlignment" use="optional"/>
						<xs:attribute name="asComboBox" type="xs:boolean" use="optional" default="false"/>
						<xs:attribute name="foreignAlias" type="xs:string" use="required"/>
						<xs:attribute name="foreignTableField" type="xs:string" use="required"/>
						<xs:attribute name="relationToUse" type="xs:string" use="required"/>
						<xs:attribute name="browserToUse" type="xs:string" use="required"/>
						<xs:attribute name="relationset" type="xs:string" use="required"/>
						<xs:attribute name="filldirection" type="CastorFilldirection" use="required"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="InFormBrowser">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
							<xs:element name="Caption" type="CastorCaption" minOccurs="0"/>
							<!-- seit 2.7.4 -->
							<xs:element name="SelectionActionEventHandler" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
									</xs:sequence>
									<xs:attribute name="className" type="xs:string" use="required"/>
									<xs:attribute name="label" type="xs:string" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<!-- Leiche. kann entfallen. Wird im Designer nur noch weg abwärts.komp. gesetzt.
						     Bei einer M-N Relation wird diese dann nicht mehr gesetzt
						--> 
						<xs:attribute name="relationToUse" type="xs:string" use="optional"/>
						<xs:attribute name="browserToUse" type="xs:string" use="required"/>
						<xs:attribute name="newMode" type="xs:boolean" use="required"/>
						<xs:attribute name="updateMode" type="xs:boolean" use="required"/>
						<xs:attribute name="searchMode" type="xs:boolean" use="optional" default="true"/>
						<!-- Veraltet. Wird durch die beiden Attribute deleteModeSelected, deleteModeUpdateNew ersetzt.
						     In der Engine wird dies jedoch noch ausgewertet=> Alte Applikation laufen noch.
						     Im Designer wird dies jedoch automatisch zu den neuen Attribut konvertiert.
						  -->
						<xs:attribute name="deleteMode" type="xs:boolean" use="optional" default="false"/>
						<xs:attribute name="deleteModeSelected"  type="xs:boolean" use="optional" default="false"/>
						<xs:attribute name="deleteModeUpdateNew" type="xs:boolean" use="optional" default="false"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="TableListBox">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
							<xs:element name="Caption" type="CastorCaption" minOccurs="0"/>
							<!--  seit 2.8.3  -->
              <xs:element name="Font" type="CastorFont" minOccurs="0"/>
						</xs:sequence>
         		<xs:attribute name="halign" type="CastorHorizontalAlignment" use="optional"/>
						<xs:attribute name="tableAlias" type="xs:string" use="required"/>
						<xs:attribute name="displayField" type="xs:string" use="required"/>
						<xs:attribute name="deleteModeSelected" type="xs:boolean" use="optional" default="false"/>
						<xs:attribute name="deleteModeUpdateNew" type="xs:boolean" use="optional" default="false"/>
						<!-- seit 2.8.2
						     Falls man in der Tablelistbox eine Sortierung möchte, dann hängt man einen Browser an die
						     Listbox welcher dann die entsprechende Sortierung hat.
						 -->
						<xs:attribute name="browserToUse" type="xs:string" use="optional"/>
						
             <!-- seit 2.10
                 Man kann einer TableListBox sagen ob und wie diese sich an ihren Inhalt anpassen soll
             -->
						<xs:attribute name="resizeMode" type="CastorResizeMode" use="optional" />

					</xs:complexType>
				</xs:element>
				<xs:element name="FlowLayoutContainer">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
							<xs:element name="GuiElement" type="CastorGuiElement" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
						<xs:attribute name="orientation" use="optional" default="horizontal">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="horizontal"/>
									<xs:enumeration value="vertical"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Button" type="CastorButton"/>
				<xs:element name="Label">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Caption" type="CastorCaption"/>
						</xs:sequence>
						<xs:attribute name="tableField" type="xs:string" use="optional"/>
						<!-- seit 2.10.0
						        Falls in einem Datenbank gebundenem Label NULL enthalten ist, kann hier ein sinvoller Wert hinterlegt
						        werden. Somit wird dann nicht "" sondern z.B. "-keine-" angezeigt. Kann auch einen I18N Schlüssel enthalten
						        Macht nur in Zusammenhang mit eine DB-Label Sinn.
						-->
						<xs:attribute name="nullDefaultValue" type="xs:string" use="optional"/>
						
					</xs:complexType>
				</xs:element>

				<xs:element name="PluginComponent">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
							<xs:element name="Property"  type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
						<xs:attribute name="writeContainer" type="xs:boolean" use="optional" default="true"/>
  					<xs:attribute name="pluginImplClass" type="xs:string"/>
  					<xs:attribute name="pluginId" type="xs:string" use="optional" />
  					<xs:attribute name="pluginVersion" type="xs:string" use="optional" />
					</xs:complexType>
				</xs:element>

				<xs:element name="OwnDrawElement">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>

				<xs:element name="BarChart">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
						</xs:sequence>
						<xs:attribute name="title" type="xs:boolean" use="optional" default="true"/>
						<xs:attribute name="legendX" type="xs:boolean" use="optional" default="true"/>
						<xs:attribute name="legendY" type="xs:boolean" use="optional" default="true"/>
						<xs:attribute name="background" type="xs:boolean" use="optional" default="true"/>
						<xs:attribute name="grid" type="xs:boolean" use="optional" default="true"/>
					</xs:complexType>
				</xs:element>

				<xs:element name="LineChart">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
						</xs:sequence>
						<xs:attribute name="title" type="xs:boolean" use="optional" default="true"/>
						<xs:attribute name="legendX" type="xs:boolean" use="optional" default="true"/>
						<xs:attribute name="legendY" type="xs:boolean" use="optional" default="true"/>
						<xs:attribute name="background" type="xs:boolean" use="optional" default="true"/>
						<xs:attribute name="grid" type="xs:boolean" use="optional" default="true"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="TimeLine">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Calendar">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="StyledText">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
						</xs:sequence>
						<xs:attribute name="i18nkey" type="xs:string" use="optional"/>
						<xs:attribute name="postProcessWithLetterEngine" type="xs:boolean" use="optional" default="false"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="BreadCrumb">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Caption" type="CastorCaption"/>
							<!--  seit 2.8.3  -->
              <xs:element name="Font" type="CastorFont" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="delimiter" type="xs:string" use="optional" default="/"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="StaticImage">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
						</xs:sequence>
						<xs:attribute name="src" type="xs:string" use="required"/>
						<xs:attribute name="tooltip" type="xs:string" use="optional"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="DynamicImage">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
						</xs:sequence>
						<xs:attribute name="tooltip" type="xs:string" use="optional"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="Container">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dimension" type="CastorDimension"/>
							<xs:element name="Pane" type="CastorGroup" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
	       	   	<!-- seit 2.10.0 -->
			         <xs:attribute name="resizeMode" type="CastorResizeMode" use="optional" />
							<xs:attribute name="layout" use="optional" default="tab_strip">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="tab_strip"/>
									<xs:enumeration value="stack"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Heading">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Caption" type="CastorCaption"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
 		  <!-- since 2.10 -->
		  <xs:element name="workFlow" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="visible" type="xs:boolean" use="required"/>
		<xs:attribute name="tabIndex" type="xs:nonNegativeInteger" use="optional" default="0"/>
		<xs:attribute name="paneIndex" type="xs:nonNegativeInteger" use="optional" default="0"/>
		<xs:attribute name="eventHandler" type="xs:string" use="optional"/>
		<xs:attribute name="description" type="xs:string" use="optional"/>

		<!-- since 2.7.2 -->
		<xs:attribute name="inputHint" type="xs:string" use="optional"/>

	  <!-- seit 2.8.6 -->
		<xs:attribute name="borderWidth" type="xs:integer" use="optional" default="-1"/>
		<xs:attribute name="borderColor" type="xs:string" use="optional" />
		<xs:attribute name="backgroundColor" type="xs:string" use="optional"/>

	</xs:complexType>
	<xs:complexType name="CastorApplication">
		<xs:sequence>
			<xs:element name="Domain" type="CastorDomainRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
 		  <!-- since 2.10 -->
		  <xs:element name="workFlow" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="title" type="xs:string" use="required"/>
		<xs:attribute name="eventHandler" type="xs:string"/>
		<xs:attribute name="dataScope" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="group"/>
					<xs:enumeration value="form"/>
					<xs:enumeration value="domain"/>
					<xs:enumeration value="application"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="eventHandlerLookupMethod">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="name"/>
					<xs:enumeration value="reference"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="description" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CastorDomainRef">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="externalModule" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="CastorName">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="StringValue">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:complexType name="Location">
		<xs:all>
			<xs:element name="x" type="xs:integer"/>
			<xs:element name="y" type="xs:int"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CastorProperty">
		<xs:annotation>
			<xs:documentation>Property to add data independant from XSD</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="CastorName"/>
			<xs:element name="Value" type="StringValue"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CastorDataSource">
		<xs:sequence>
			<xs:element name="Desc" type="xs:string" minOccurs="0"/>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="description" type="xs:string" use="optional"/>
		<xs:attribute name="reconfigure" use="optional" default="full">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="full"/>
					<xs:enumeration value="none"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="type" use="optional" default="sql">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="sql"/>
					<xs:enumeration value="index"/>
					<xs:enumeration value="directory"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="DocumentField">
		<xs:sequence>
			<xs:element name="Dummy" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CastorActivityDiagram">
		<xs:sequence>
			<xs:element name="DiagramNode" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Dimension" type="CastorDimension"/>
						<xs:choice>
							<xs:element name="Activity">
								<xs:complexType>
									<xs:attribute name="relatedForm" type="xs:string"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="ForkJoin">
								<xs:complexType/>
							</xs:element>
							<xs:element name="BranchMerge">
								<xs:complexType/>
							</xs:element>
							<xs:element name="Start">
								<xs:complexType/>
							</xs:element>
							<xs:element name="End">
								<xs:complexType/>
							</xs:element>
						</xs:choice>
						<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="name" type="xs:string" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Transition" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="fromNode" type="xs:string" use="required"/>
					<xs:attribute name="toNode" type="xs:string" use="required"/>
					<xs:attribute name="description" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="StringValue" use="required"/>
		<xs:attribute name="createDate" type="xs:dateTime"/>
	</xs:complexType>
	<xs:simpleType name="CastorVerticalAlignment">
		<xs:restriction base="xs:string">
			<xs:enumeration value="top"/>
			<xs:enumeration value="middle"/>
			<xs:enumeration value="bottom"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CastorResizeMode">
		<xs:restriction base="xs:string">
			<xs:enumeration value="none"/>
			<xs:enumeration value="booth"/>
			<xs:enumeration value="width"/>
			<xs:enumeration value="height"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="CastorHorizontalAlignment">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CastorRole">
		<xs:sequence>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="description" type="xs:string"/>
	</xs:complexType>
</xs:schema>
