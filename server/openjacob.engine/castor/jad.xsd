<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:element name="Jacob">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DataSources">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DataSource" type="CastorDataSource" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Roles" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Sollte beim nächsten Majorrelease zu Pflicht gemacht werden!</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Role" type="CastorRole" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Tables">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Table" type="CastorTable" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="TableAliases">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TableAlias" type="CastorTableAlias" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Browsers">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Browser" type="CastorBrowser" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Relations">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Relation" type="CastorRelation" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Relationsets">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Relationset" type="CastorRelationset" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Forms">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Form" type="CastorJacobForm" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="ExternalForm" type="CastorExternalForm" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="MutableForm" type="CastorMutableForm" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="HtmlForm" type="CastorHtmlForm" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Domains">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Domain" type="CastorDomain" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Applications">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Application" type="CastorApplication" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ExternalModules" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Kann beim nächsten Majorrelease gelöscht werden. Wird nicht mehr benötigt!</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ExternalModule" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="filename" type="xs:string" use="required"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Diagrams" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ActivityDiagram" type="CastorActivityDiagram" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="version" type="xs:string" use="required"/>
			<xs:attribute name="engineVersion" type="xs:string" use="required"/>
			<xs:attribute name="defaultApplication" type="xs:string" use="optional"/>
			<xs:attribute name="erroneous" type="xs:boolean" use="optional" default="false"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CastorTable">
		<xs:sequence>
			<xs:element name="Field" type="CastorTableField" maxOccurs="unbounded"/>
			<xs:element name="PrimaryKey" type="CastorKey" minOccurs="0">
				<xs:annotation>
					<xs:documentation>TODO: Prüfen ob dieses Element nicht Pflicht sein kann!</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ForeignKey" type="CastorKey" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="UniqueIndex" type="CastorKey" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Index" type="CastorKey" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="datasource" type="xs:string" use="required"/>
		<xs:attribute name="dbName" type="xs:string" use="optional"/>
		<xs:attribute name="representativeField" type="xs:string" use="optional"/>
		<xs:attribute name="historyField" type="xs:string" use="optional"/>
		<xs:attribute name="description" type="xs:string" use="optional"/>
		<xs:attribute name="omitLocking" type="xs:boolean" use="optional" default="false"/>
		<!-- since 2.9.4 -->
		<xs:attribute name="NtoMTable" type="xs:boolean" use="optional" default="false"/>
		<!-- since 2.9.4 -->
		<xs:attribute name="recordsAlwaysDeletable" type="xs:boolean" use="optional" default="false"/>
	</xs:complexType>
	<xs:complexType name="CastorTableField">
		<xs:sequence>
			<xs:choice>
				<xs:element name="Text" type="TextField"/>
				<xs:element name="Boolean" type="BooleanField">
					<xs:annotation>
						<xs:documentation>Seit jACOB 2.6</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Integer" type="IntegerField"/>
				<xs:element name="Long" type="LongField"/>
				<xs:element name="Float" type="FloatField"/>
				<xs:element name="Double" type="DoubleField"/>
				<xs:element name="Decimal" type="DecimalField"/>
				<xs:element name="Date" type="DateField" nillable="true"/>
				<xs:element name="Time" type="TimeField"/>
				<xs:element name="Timestamp" type="TimestampField"/>
				<xs:element name="LongText" type="LongTextField"/>
				<xs:element name="Binary" type="BinaryField"/>
				<xs:element name="Enumeration" type="EnumerationField"/>
				<xs:element name="Document" type="DocumentField"/>
			</xs:choice>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="dbName" type="xs:string" use="optional"/>
		<xs:attribute name="required" type="xs:boolean" use="required"/>
		<xs:attribute name="readonly" type="xs:boolean" use="required"/>
		<xs:attribute name="customFieldType" type="xs:string" use="optional"/>
		<xs:attribute name="history" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="label" type="xs:string" use="optional"/>
		<xs:attribute name="description" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CastorTableAlias">
		<xs:sequence>
			<xs:element name="Condition" type="xs:string" minOccurs="0"/>
			<xs:element name="SortField" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:attribute name="fieldName" type="xs:string" use="required"/>
					<xs:attribute name="sortOrder" type="CastorSortOrder" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="table" type="xs:string" use="required"/>
		<xs:attribute name="description" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="CastorBrowser">
		<xs:sequence>
			<xs:element name="Field" type="CastorBrowserField" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
			
			<!-- since 2.7.4 -->
			<!-- implements a tree with a CONNECT BY table field known from Oracel -->
      <xs:element name="ConnectByKey" type="CastorKey"  minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="alias" type="xs:string" use="required"/>
		<xs:attribute name="description" type="xs:string" use="optional"/>
		<xs:attribute name="type" type="BrowserType" use="required"/>
	</xs:complexType>
	<xs:complexType name="CastorBrowserField">
		<xs:sequence>
			<xs:choice>
				<xs:element name="TableField" type="CastorBrowserTableFieldType"/>
				<xs:element name="Runtime" type="CastorBrowserRuntimeFieldType"/>
			</xs:choice>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="label" type="xs:string" use="required"/>
		<xs:attribute name="visible" type="xs:boolean" use="required"/>
		<xs:attribute name="configureable" type="xs:boolean" use="optional" default="true"/>
		<xs:attribute name="readonly" type="xs:boolean" use="optional" default="false"/>
	</xs:complexType>
	<xs:complexType name="CastorBrowserTableFieldType">
		<xs:sequence>
			<xs:element name="Foreign" type="CastorBrowserForeignType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="tableField" type="xs:string" use="required"/>
		<xs:attribute name="sortOrder" type="CastorSortOrder" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CastorBrowserForeignType">
		<xs:attribute name="foreignAlias" type="xs:string" use="required"/>
		<xs:attribute name="relationToUse" type="xs:string" use="optional"/>
		<xs:attribute name="browserToUse" type="xs:string" use="optional"/>
		<xs:attribute name="relationset" type="xs:string" use="optional"/>
		<xs:attribute name="filldirection" type="CastorFilldirection" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CastorBrowserRuntimeFieldType">
		<xs:attribute name="dummy" type="xs:boolean" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CastorKey">
		<xs:sequence>
			<xs:element name="Field" type="CastorKeyField" maxOccurs="unbounded"/>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="CastorRelation">
		<xs:sequence>
			<xs:choice>
				<xs:element name="OneToMany" type="CastorOneToMany"/>
				<xs:element name="ManyToMany" type="CastorManyToMany"/>
			</xs:choice>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="CastorOneToMany">
		<xs:attribute name="fromAlias" type="xs:string" use="required"/>
		<xs:attribute name="toAlias" type="xs:string" use="required"/>
		<xs:attribute name="toKey" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="CastorManyToMany">
		<xs:attribute name="fromRelation" type="xs:string" use="required"/>
		<xs:attribute name="toRelation" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="CastorRelationset">
		<xs:sequence>
			<xs:element name="Relation" type="CastorName" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="CastorDimension">
		<xs:attribute name="x" type="xs:integer" use="required"/>
		<xs:attribute name="y" type="xs:integer" use="required"/>
		<xs:attribute name="height" type="xs:nonNegativeInteger" use="required"/>
		<xs:attribute name="width" type="xs:nonNegativeInteger" use="required"/>
	</xs:complexType>
	<xs:complexType name="CastorFont">
		<xs:attribute name="family" use="optional" default="sans-serif">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="serif"/>
					<xs:enumeration value="sans-serif"/>
					<xs:enumeration value="cursive"/>
					<xs:enumeration value="fantasy"/>
					<xs:enumeration value="monospace"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="style" use="optional" default="normal">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="normal"/>
					<xs:enumeration value="italic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="weight" use="optional" default="normal">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="normal"/>
					<xs:enumeration value="bold"/>
					<xs:enumeration value="bolder"/>
					<xs:enumeration value="lighter"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="size" type="xs:integer"/>
	</xs:complexType>
	<xs:complexType name="CastorButton">
		<xs:sequence>
			<xs:element name="Dimension" type="CastorDimension"/>
			<xs:element name="Action" type="CastorAction"/>
			<xs:element name="Font" type="CastorFont" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="label" type="xs:string" use="required"/>
		<xs:attribute name="emphasize" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="default" type="xs:boolean" use="optional"/>
		<xs:attribute name="halign" type="CastorHorizontalAlignment" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CastorCaption">
		<xs:sequence>
			<xs:element name="Dimension" type="CastorDimension" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Some GUI Elements such like InFormBrowser do not need a dimension for caption!</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Action" type="CastorAction" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A caption might be implemented as a link to execute an action</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Property" type="CastorProperty" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Font" type="CastorFont" minOccurs="0"/>
      <!-- since 2.10 -->
		  <xs:element name="workFlow" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<!-- since 2.8.6 -->
		<xs:attribute name="color" type="xs:string" use="optional"/>
		<xs:attribute name="ellipsis" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="label" type="xs:string" use="required"/>
		<xs:attribute name="halign" type="CastorHorizontalAlignment" default="right"/>
		<xs:attribute name="valign" type="CastorVerticalAlignment" default="top"/>
		<!-- since 2.7.2 -->
		<xs:attribute name="eventHandler" type="xs:string" use="optional"/>
					
	</xs:complexType>
	<xs:complexType name="CastorAction">
		<xs:choice>
			<xs:element name="Generic">
				<xs:complexType>
					<xs:attribute name="dummy" type="xs:boolean" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="NewRecord">
				<xs:complexType>
					<xs:attribute name="dummy" type="xs:boolean" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="UpdateRecord">
				<xs:complexType>
					<xs:attribute name="changeUpdate" type="xs:boolean" use="required"/>
					<xs:attribute name="executeScope" use="optional" default="outer group">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="outer group"/>
								<xs:enumeration value="tab pane"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="DeleteRecord">
				<xs:complexType>
					<xs:attribute name="needUserConfirmation" type="xs:boolean" use="optional" default="true"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="ClearGroup">
				<xs:complexType>
					<xs:attribute name="dummy" type="xs:boolean" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Search">
				<xs:complexType>
					<xs:attribute name="relationset" type="xs:string" use="required"/>
					<xs:attribute name="filldirection" type="CastorFilldirection" use="required"/>
					<xs:attribute name="safeMode" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="SearchUpdateRecord">
				<xs:complexType>
					<xs:attribute name="relationset" type="xs:string" use="required"/>
					<xs:attribute name="filldirection" type="CastorFilldirection" use="required"/>
					<xs:attribute name="safeMode" type="xs:boolean" use="required"/>
					<xs:attribute name="changeUpdate" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="RecordSelected">
				<xs:complexType>
					<xs:attribute name="dummy" type="xs:boolean" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="NavigateToForm">
				<xs:complexType>
					<xs:attribute name="formName" type="xs:string" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TextField">
		<xs:sequence>
			<xs:element name="Default" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="maxLength" type="xs:nonNegativeInteger" use="required"/>
		<xs:attribute name="caseSensitive" type="xs:boolean" use="required"/>
		<xs:attribute name="searchMode" type="TextFieldSearchModeType" use="required"/>
		<xs:attribute name="fixedLength" type="xs:boolean" use="optional" default="false"/>
	</xs:complexType>
	<xs:complexType name="BooleanField">
		<xs:sequence>
			<xs:element name="Default" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IntegerField">
		<xs:sequence>
			<xs:element name="Default" type="xs:integer" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="autoincrement" type="xs:boolean" use="required"/>
		<xs:attribute name="min" type="xs:integer" use="optional"/>
		<xs:attribute name="max" type="xs:integer" use="optional"/>
		<xs:attribute name="dbincrement" type="xs:boolean" use="optional" default="false"/>
	</xs:complexType>
	<xs:complexType name="LongField">
		<xs:sequence>
			<xs:element name="Default" type="xs:long" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="autoincrement" type="xs:boolean" use="required"/>
		<xs:attribute name="min" type="xs:long" use="optional"/>
		<xs:attribute name="max" type="xs:long" use="optional"/>
		<xs:attribute name="dbincrement" type="xs:boolean" use="optional" default="false"/>
	</xs:complexType>
	<xs:complexType name="FloatField">
		<xs:sequence>
			<xs:element name="Default" type="xs:float" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="min" type="xs:float" use="optional"/>
		<xs:attribute name="max" type="xs:float" use="optional"/>
	</xs:complexType>
	<xs:complexType name="DoubleField">
		<xs:sequence>
			<xs:element name="Default" type="xs:double" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="min" type="xs:double" use="optional"/>
		<xs:attribute name="max" type="xs:double" use="optional"/>
	</xs:complexType>
	<xs:complexType name="DecimalField">
		<xs:sequence>
			<xs:element name="Default" type="xs:decimal" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="min" type="xs:decimal" use="optional"/>
		<xs:attribute name="max" type="xs:decimal" use="optional"/>
		<xs:attribute name="scale" type="xs:short" use="optional" default="2"/>
	</xs:complexType>
	<xs:complexType name="DateField">
		<xs:sequence>
			<xs:element name="Default" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimeField">
		<xs:sequence>
			<xs:element name="Default" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimestampField">
		<xs:sequence>
			<xs:element name="Default" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="resolution" use="optional" default="secbase">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="minbase"/>
					<xs:enumeration value="secbase"/>
					<xs:enumeration value="msecbase"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="LongTextField">
		<xs:sequence>
			<xs:element name="Default" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="changeHeader" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="disableMapping" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="editMode" type="LongTextEditModeType" use="optional"/>
		<xs:attribute name="sqlType" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="LONGVARCHAR"/>
					<xs:enumeration value="CLOB"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BinaryField">
		<xs:sequence>
			<xs:element name="Default" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="disableMapping" type="xs:boolean" use="optional" default="false"/>
		<xs:attribute name="sqlType" use="optional">
			<xs:simpleType>
