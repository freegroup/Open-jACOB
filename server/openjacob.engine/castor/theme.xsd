<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/ruleset" targetNamespace="http://www.example.org/ruleset">
	<element name="CastorTheme">
		<complexType>
			<sequence>
				<element name="CastorNavigation" maxOccurs="1" minOccurs="1">
					<complexType>
						<attribute name="width" type="integer"></attribute>
					</complexType>
				</element>
			</sequence>
			<attribute name="displayName"   type="string" use="required"/>
			<attribute name="engineVersion" type="string" use="required"/>
		</complexType>
	</element>
</schema>
