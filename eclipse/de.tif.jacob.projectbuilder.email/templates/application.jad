        <Text caseSensitive="false" maxLength="250" searchMode="unbound"/>
      </Field>
      <Field dbName="answered" history="false" label="Answered" name="answered" readonly="false" required="false">
        <Integer autoincrement="false" max="1" min="0">
          <Default>0</Default>
        </Integer>
