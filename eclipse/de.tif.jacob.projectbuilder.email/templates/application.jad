  <TableAliases>
    <TableAlias name="email" table="email">
      <Condition>email.mandator_id='{MANDATORID}'</Condition>
    </TableAlias>
    <TableAlias name="configuration" table="configuration">
      <Condition>configuration.mandator_id='{MANDATORID}'</Condition>
    </TableAlias>
    <TableAlias name="folder" table="folder">
      <Condition>folder.mandator_id='{MANDATORID}'</Condition>
    </TableAlias>
    <TableAlias name="attachment" table="attachment">
      <Condition>attachment.mandator_id='{MANDATORID}'</Condition>
    </TableAlias>
    <TableAlias name="contact" table="contact">
      <Condition>contact.mandator_id='{MANDATORID}'</Condition>
    </TableAlias>
    <TableAlias name="person" table="person">
      <Condition/>
    </TableAlias>
    <TableAlias name="account" table="person">
      <Condition>account.loginname!='admin'</Condition>
    </TableAlias>
    <TableAlias name="privatContact" table="contact">
      <Condition>privatContact.type='privat' AND privatContact.mandator_id='{MANDATORID}'</Condition>
    </TableAlias>
    <TableAlias name="commonContact" table="contact">
      <Condition>commonContact.type='common' AND commonContact.mandator_id='{MANDATORID}'</Condition>
    </TableAlias>
    <TableAlias name="send_email" table="email">
      <Condition>send_email.mandator_id='{MANDATORID}'</Condition>
    </TableAlias>
  </TableAliases>
