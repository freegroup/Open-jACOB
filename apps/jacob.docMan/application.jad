      <Field dbName="tablealias" history="false" label="Tablealias" name="tablealias" readonly="true" required="true">
        <LongText editMode="fulledit"/>
        <Property>
          <Name>DESCRIPTION</Name>
          <Value>Table alias of the matching data table record</Value>
        </Property>
      </Field>
      <Field dbName="tablename" history="false" label="Tablename" name="tablename" readonly="true" required="true">
        <LongText editMode="fulledit"/>
        <Property>
          <Name>DESCRIPTION</Name>
          <Value>Table name of the matching data table record</Value>
        </Property>
      </Field>
      <Field dbName="primarykey" history="false" label="Primarykey" name="primarykey" readonly="true" required="true">
        <LongText editMode="fulledit"/>
        <Property>
          <Name>DESCRIPTION</Name>
