      </Field>
      <Field dbName="created" label="Created" name="created" readonly="true" required="true">
        <Timestamp resolution="minbase">
          <Default>now</Default>
        </Timestamp>
      </Field>
      <Field dbName="lastexecution" label="Last execution" name="lastexecution" readonly="true" required="false">
        <Timestamp resolution="secbase"/>
      </Field>
      <Field dbName="nextexecution" label="Next execution" name="nextexecution" readonly="true" required="false">
        <Timestamp resolution="secbase"/>
      </Field>
      <Field dbName="taskstatus" label="Status" name="taskstatus" readonly="false" required="true">
        <Enumeration>
          <Value>scheduled</Value>
          <Value>hibernated</Value>
        </Enumeration>
      </Field>
      <Field dbName="duration_ms" label="Duration" name="duration_ms" readonly="true" required="false">
        <Integer autoincrement="false" dbincrement="false"/>
      </Field>
      <Field dbName="errorMessage" label="Error message" name="errorMessage" readonly="true" required="false">
        <Text caseSensitive="false" fixedLength="false" maxLength="500" searchMode="unbound"/>
      </Field>
      <Field dbName="name" label="Name" name="name" readonly="true" required="true">
        <Text caseSensitive="false" fixedLength="false" maxLength="128" searchMode="unbound"/>
      </Field>
      <Field dbName="applicationname" label="Application" name="applicationname" readonly="true" required="true">
        <Text caseSensitive="false" fixedLength="false" maxLength="40" searchMode="unbound"/>
      </Field>
      <Field dbName="applicationversion" label="Version" name="applicationversion" readonly="true" required="false">
        <Text caseSensitive="false" fixedLength="false" maxLength="20" searchMode="unbound"/>
      </Field>
      <Field dbName="ownerid" label="Ownerid" name="ownerid" readonly="true" required="false">
        <Text caseSensitive="false" fixedLength="false" maxLength="50" searchMode="unbound"/>
      </Field>
      <Field dbName="taskdetails" label="Taskdetails" name="taskdetails" readonly="true" required="false">
        <Text caseSensitive="false" fixedLength="false" maxLength="80" searchMode="unbound"/>
      </Field>
      <Field dbName="scope" label="Scope" name="scope" readonly="true" required="false">
        <Enumeration>
          <Value>cluster</Value>
          <Value>node</Value>
          <Value>session</Value>
        </Enumeration>
      </Field>
      <PrimaryKey name="prim">
        <Field>schedulerid</Field>
        <Field>taskid</Field>
      </PrimaryKey>
    </Table>
    <Table datasource="jacobPersistentDataSource" dbName="enginetaskstatus" description="persistent task status" name="enginetaskstatus">
      <Field dbName="taskstatus" label="Taskstatus" name="taskstatus" readonly="true" required="true">
        <Enumeration>
          <Value>scheduled</Value>
          <Value>hibernated</Value>
        </Enumeration>
      </Field>
      <Field dbName="applicationname" label="Applicationname" name="applicationname" readonly="true" required="true">
        <Text caseSensitive="false" fixedLength="false" maxLength="40" searchMode="unbound"/>
      </Field>
      <Field dbName="taskname" label="Taskname" name="taskname" readonly="true" required="true">
        <Text caseSensitive="false" fixedLength="false" maxLength="128" searchMode="unbound"/>
      </Field>
      <PrimaryKey name="prim">
        <Field>applicationname</Field>
        <Field>taskname</Field>
