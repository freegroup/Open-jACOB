  <Roles/>
  <Tables>
    <Table datasource="jacobPersistentDataSource" dbName="yan_queue" description="Queue for the jAN" name="jan_queue">
      <Field dbName="sender" label="Sender" name="sender" readonly="false" required="false">
        <Text caseSensitive="false" fixedLength="false" maxLength="200" searchMode="unbound"/>
      </Field>
      <Field dbName="senderAddr" history="false" label="Sender address" name="senderAddr" readonly="false" required="false">
        <Text caseSensitive="false" maxLength="200" searchMode="unbound"/>
      </Field>
      <Field dbName="message" label="Message" name="message" readonly="false" required="true">
        <LongText changeHeader="true"/>
      </Field>
      <Field dbName="state" label="Status" name="state" readonly="false" required="true">
        <Enumeration>
          <Default>New</Default>
          <Value>New</Value>
          <Value>Error</Value>
        </Enumeration>
      </Field>
      <Field dbName="errorMessage" label="Error message" name="errorMessage" readonly="true" required="false">
        <Text caseSensitive="false" fixedLength="false" maxLength="500" searchMode="unbound"/>
      </Field>
      <Field dbName="sourceApp" label="Application" name="sourceApp" readonly="true" required="true">
        <Text caseSensitive="false" fixedLength="false" maxLength="40" searchMode="unbound"/>
      </Field>
      <Field dbName="pkey" label="ID" name="pkey" readonly="true" required="true">
        <Integer autoincrement="true" dbincrement="false"/>
      </Field>
      <Field dbName="url" label="URL" name="url" readonly="false" required="true">
        <Text caseSensitive="false" fixedLength="false" maxLength="255" searchMode="unbound"/>
      </Field>
      <Field dbName="urlComplete" history="false" label="URL" name="urlComplete" readonly="false" required="false">
        <LongText editMode="fulledit"/>
        <Property>
          <Name>DESCRIPTION</Name>
          <Value>since jACOB 2.9 to have no limitation for amount of recipients</Value>
        </Property>
      </Field>
      <Field dbName="created" label="Created" name="created" readonly="true" required="true">
        <Timestamp resolution="minbase">
          <Default>now</Default>
        </Timestamp>
      </Field>
      <Field dbName="errorAt" history="false" label="Error time" name="errorAt" readonly="true" required="false">
        <Timestamp resolution="secbase"/>
      </Field>
      <Field dbName="tries" history="false" label="Tries" name="tries" readonly="false" required="true">
        <Integer autoincrement="false" min="0">
          <Default>0</Default>
        </Integer>
        <Property>
          <Name>DESCRIPTION</Name>
          <Value>Tries how often sending has already failed</Value>
        </Property>
      </Field>
      <PrimaryKey name="pkey">
        <Field>pkey</Field>
      </PrimaryKey>
    </Table>
    <Table datasource="jacobPersistentDataSource" dbName="yan_attachment" name="jan_attachment">
      <Field dbName="message_key" history="false" label="Message" name="message_key" readonly="true" required="true">
        <Integer autoincrement="false"/>
      </Field>
      <Field dbName="attachnbr" history="false" label="Nbr." name="attachnbr" readonly="true" required="true">
        <Integer autoincrement="false" min="1"/>
      </Field>
      <Field dbName="document" history="false" label="Document" name="document" readonly="true" required="true">
        <Document/>
      </Field>
      <Field dbName="mimetype" history="false" label="Mime type" name="mimetype" readonly="true" required="false">
        <Text caseSensitive="false" maxLength="40" searchMode="unbound"/>
      </Field>
      <PrimaryKey name="primaryKey">
        <Field>message_key</Field>
        <Field>attachnbr</Field>
      </PrimaryKey>
      <ForeignKey name="jan_queue_FKey">
        <Field>message_key</Field>
      </ForeignKey>
    </Table>
    <Table datasource="jacobTransientDataSource" dbName="enginetask" description="transient engine task entries" name="enginetask">
      <Field dbName="taskid" label="Task-ID" name="taskid" readonly="true" required="true">
        <Text caseSensitive="false" fixedLength="true" maxLength="40" searchMode="unbound"/>
      </Field>
      <Field dbName="schedulerid" label="Scheduler-ID" name="schedulerid" readonly="true" required="true">
