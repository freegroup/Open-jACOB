      <Field dbName="defOriginator" history="false" label="Default Originator" name="defOriginator" readonly="false" required="false">
        <Text caseSensitive="false" maxLength="80" searchMode="unbound"/>
      </Field>
      <Field dbName="defOriginatorAddr" history="false" label="Default Originator Address" name="defOriginatorAddr" readonly="false" required="true">
        <Text caseSensitive="false" maxLength="80" searchMode="unbound"/>
      </Field>
      <Field dbName="maxRetries" history="false" label="Max. Retries" name="maxRetries" readonly="false" required="true">
        <Enumeration>
          <Default>5</Default>
          <Value>1</Value>
          <Value>5</Value>
          <Value>10</Value>
          <Label>1</Label>
          <Label>5</Label>
          <Label>10</Label>
        </Enumeration>
      </Field>
      <Field dbName="retryTimespan" history="false" label="Retry Timespan" name="retryTimespan" readonly="false" required="true">
        <Enumeration>
          <Default>1</Default>
          <Value>1</Value>
          <Value>5</Value>
          <Value>10</Value>
          <Value>30</Value>
          <Label>1</Label>
          <Label>5</Label>
          <Label>10</Label>
          <Label>30</Label>
        </Enumeration>
        <Property>
          <Name>DESCRIPTION</Name>
          <Value>Retry timespan in mins</Value>
        </Property>
      </Field>
