DEFINE TABLESET agent WITH TABLES
	employee ALIAS agent
		CONDITION "agent.supportrole = 1"
		TABLERULE BEFOREUPDATE SCRIPT "EmployeeUpdate" FROM FILE "person.qsc";
DEFINE TABLESET appprofile WITH TABLES
	employee,
	workgroup ALIAS callworkgroup
		DESC = "Auftragskoordinator"
		CONDITION "callworkgroup.wrkgrptype=0 OR callworkgroup.wrkgrptype=3 ",
	qw_reports ALIAS qw_reportscall,
	qw_reports,
	workgroup ALIAS problemmanager
		CONDITION "problemmanager.wrkgrptype=0",
	contract
		DESC = "Standardvertrag zur Bestimmung von Vertragsleistungen",
	appprofile,
	process;
DEFINE TABLESET attachment WITH TABLES
	attachment ALIAS attachmentadd,
	attachment,
	calls ALIAS call
		TABLERULE BEFOREUPDATE SCRIPT "CallUpdate" FROM FILE "call.qsc"
		TABLERULE AFTERUPDATE SCRIPT "CallPostUpdate" FROM FILE "call.qsc"
		TABLERULE BEFORESEARCH SCRIPT "SafeSearch" FROM FILE "call.qsc",
	task;
DEFINE TABLESET call WITH TABLES
	category
		CONDITION "category.categorystatus<>7",
	employee ALIAS customerint
		CONDITION "customerint.employeestatus = 0 Or customerint.employeestatus = 1",
	calls ALIAS callduplicate
		CONDITION "callduplicate.mastercall_key > 0 ",
	calls ALIAS call
		TABLERULE BEFOREUPDATE SCRIPT "CallUpdate" FROM FILE "call.qsc"
		TABLERULE AFTERUPDATE SCRIPT "CallPostUpdate" FROM FILE "call.qsc"
		TABLERULE BEFORESEARCH SCRIPT "SafeSearch" FROM FILE "call.qsc";
DEFINE TABLESET calldup WITH TABLES
	calls ALIAS callduplicate
		CONDITION "callduplicate.mastercall_key > 0 ",
	calls ALIAS call
		TABLERULE BEFOREUPDATE SCRIPT "CallUpdate" FROM FILE "call.qsc"
		TABLERULE AFTERUPDATE SCRIPT "CallPostUpdate" FROM FILE "call.qsc"
		TABLERULE BEFORESEARCH SCRIPT "SafeSearch" FROM FILE "call.qsc",
	calls ALIAS callmaster,
	workgroup ALIAS calldupworkgroup
		DESC = "Auftragskoordinator"
		CONDITION "calldupworkgroup.wrkgrptype=0";
DEFINE TABLESET callduration WITH TABLES
	callduration ALIAS q_callduration,
	callduration,
	workgroup ALIAS dur_workgroup,
	workgroup ALIAS oldgroup,
	calls ALIAS call,
	workgroup ALIAS newgroup;
