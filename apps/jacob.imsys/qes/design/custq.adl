DEFINE TABLESET calls_int WITH TABLES
	employee ALIAS affectedperson
		CONDITION "affectedperson.employeestatus = 0 Or affectedperson.employeestatus = 1",
	callinfo,
	employee ALIAS customerint
		CONDITION "customerint.employeestatus = 0 Or customerint.employeestatus = 1"
		TABLERULE BEFOREUPDATE SCRIPT "EmployeeUpdate" FROM FILE "person.qsc",
	category
		CONDITION "category.categorystatus<>7",
	calls ALIAS callduplicate
		CONDITION "callduplicate.mastercall_key > 0 ",
	custcall,
	employee ALIAS dupcustomer,
	object
		CONDITION "object.objstatus < 5 ",
	calls ALIAS call
		TABLERULE BEFOREUPDATE SCRIPT "CallUpdate" FROM FILE "call.qsc"
		TABLERULE AFTERUPDATE SCRIPT "CallPostUpdate" FROM FILE "call.qsc"
