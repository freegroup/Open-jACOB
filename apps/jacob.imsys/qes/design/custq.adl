		CONDITION "category.categorystatus<>7",
	callescalation
		TABLERULE BEFOREUPDATE SCRIPT "CallEscalationUpdate" FROM FILE "action.qsc",
	doc_template,
	callaction
		TABLERULE BEFOREUPDATE SCRIPT "CallActionUpdate" FROM FILE "action.qsc",
	workgroup,
	tasktype,
	ext_system;
