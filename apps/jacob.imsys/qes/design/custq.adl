DEFINE TABLE callaction WITH DATASOURCE custqDataSource
	DESC "Notification action rules for call records"
	PRIMARY KEY PKey("pkey")
	FOREIGN KEY categorycallaction_FKey("categoryaction")
	FOREIGN KEY doc_template_FKey("doc_template_key")
	FOREIGN KEY ext_system_FKey("ext_system_key")
	FOREIGN KEY tasktype_FKey("tasktype_key")
	FOREIGN KEY workgroupcallaction_FKey("groupaction")
	INFOFIELD "name"
{
	"pkey" LABEL "ID" READONLY
		SERIAL
		DESC = "Primary key.";
	"name" LABEL "Name"
		VSTRING OF LENGTH 35
		DESC = "Action rule name"
		REQUIRED;
	"subject" LABEL "Betreff"
		VSTRING OF LENGTH 240
		DESC = "Subject line for notification message";
	"method" LABEL "Methode"
		ENUM "Signal","Email","FAX","Funkruf"
		DESC = "How message is to be sent"
		DEFAULT = "Signal";
	"statusevent" LABEL "wenn Status ist"
		ENUM "Neu","Rückruf","Durchgestellt","AK zugewiesen","Fehlgeroutet","Verworfen","Angenommen","Fertig gemeldet","Fertig akzeptiert","Dokumentiert","Geschlossen","Angelegt","Freigegeben","Storniert","In Arbeit","Abgerechnet","Abgeschlossen"
		DESC = "Status value that invokes this action"
		REQUIRED;
	"recipient" LABEL "Empfänger"
		ENUM "folgende Adresse","Kunde","Mitarbeiter","Arbeitsgruppe","NM Meisterei","AK des Auftrags"
		DESC = "Who shall receive the notification?"
		REQUIRED;
	"recordinfo" LABEL "Ptext beifügen"
		INTEGER MAX "1" MIN "0"
		DESC = "Include contents of associated record?"
		DEFAULT = 0;
	"message" LABEL "Nachricht"
		TEXT
		DESC = "Body of notification message";
	"targettable" LABEL "Bezugstabelle"
		ENUM "Meldung","Auftrag","keine"
		DESC = "Table that this rule applies to"
		REQUIRED;
	"groupaction" LABEL "groupaction (Foreign Key)"
		INTEGER;
	"notificationaddr" LABEL "Nachricht an"
		VSTRING OF LENGTH 100
		DESC = "Address to use when notifying workgroup";
	"categoryaction" LABEL "categorycallact"
		INTEGER
		DESC = "Link to Category";
	"migration" LABEL "Migration"
		ENUM "Nein","Ja"
		DESC = "Ist die Meisterei migriert?"
		DEFAULT = "Ja" REQUIRED;
	"priority" LABEL "Priorität" HISTORY
		ENUM "1-Normal","2-Kritisch","3-Produktion","4-Notfall"
		DESC = "Meldungspriorität";
	"tasktype_key" LABEL "tasktype_key"
		VSTRING OF LENGTH 4;
	"ext_system_key" LABEL "ext_system_key"
		INTEGER;
	"doc_template_key" LABEL "doc_template_key"
		INTEGER;
	"confirmation" LABEL "Confirmation"
		ENUM "Nein","Ja"
		DEFAULT = "Nein";
	"excludecategory" LABEL "Excludecategory"
		VSTRING OF LENGTH 200
		DESC = "Gewerke die nicht berücksichtigt werden sollen";
};

DEFINE TABLE callduration WITH DATASOURCE custqDataSource
	DESC "Tracks the calls staying in the workgroups(AK)"
	PRIMARY KEY PKey("pkey")
	FOREIGN KEY callcallduration_FKey("callcallduration")
	FOREIGN KEY newgroupcallduration_FKey("newgroup")
	FOREIGN KEY oldgroupcallduration_FKey("oldgroup")
	FOREIGN KEY wkgpcallduration_FKey("groupcallduration")
{
	"pkey" LABEL "pkey" READONLY
		SERIAL
		DESC = "Primary key.";
	"groupcallduration" LABEL "Gruppe"
		INTEGER
		DESC = "Link to the workgroup"
		REQUIRED;
	"newgroup" LABEL "Alte Gruppe"
		INTEGER
		DESC = "Link to the new workgroup (AK)";
	"oldgroup" LABEL "Neue Gruppe"
		INTEGER
		DESC = "Link to the old workgroup (AK)";
	"callcallduration" LABEL "Meldung"
		INTEGER
		DESC = "Link to the call"
		REQUIRED;
	"date_from" LABEL "Von"
		DATETIME
		DESC = "Date and Time entering the workgroup (AK)";
	"date_until" LABEL "Bis"
		DATETIME
		DESC = "Date and Time leaving the workgroup (AK)";
	"isclosed" LABEL "isclosed"
		INTEGER MAX "1" MIN "0"
		DESC = "Flag to see if a call closed (Redundant)"
		DEFAULT = 0;
};

DEFINE TABLE callescalation WITH DATASOURCE custqDataSource
	DESC "Esclation rules for call records"
	PRIMARY KEY PKey("pkey")
	FOREIGN KEY categorycallescalation_FKey("categoryescalation")
	FOREIGN KEY escalationgroupcallescalation_FKey("escgroupescalation")
	FOREIGN KEY groupescalation_FKey("groupescalation")
	INFOFIELD "name"
{
	"pkey" LABEL "ID" READONLY
		SERIAL
		DESC = "Primary key";
	"groupescalation" LABEL "groupescalation (Foreign Key)"
		INTEGER
		DESC = "Link to groupmember table";
	"name" LABEL "Name"
		VSTRING OF LENGTH 35
		DESC = "Escalation rule name"
		REQUIRED;
	"type" LABEL "Typ"
		ENUM "nicht zugewiesen","Fehlgeroutet","nicht Angenommen","nicht fertig gemeldet"
		DESC = "Event or condition that causes rule to fire"
		REQUIRED;
	"priority" LABEL "Priorität"
		ENUM "1-Normal","2-Kritisch","3-Produktion","4-Notfall"
		DESC = "Call priority that this rule applies to";
	"delay" LABEL "Warten"
		INTEGER
		DESC = "Amount of time to wait before firing rule"
		REQUIRED;
	"delayunits" LABEL "Einheiten"
		ENUM "Minuten","Stunden"
		DESC = "Time units for waiting before firing rule"
		REQUIRED;
	"tier" LABEL "Ebene"
		INTEGER
		DESC = "Which tier of notify target will be notified"
		DEFAULT = 1 REQUIRED;
	"subject" LABEL "Betreff"
		VSTRING OF LENGTH 240
		DESC = "Notification message subject";
	"repeated" LABEL "Wiederholt"
		INTEGER MAX "1" MIN "0"
		DESC = "Is this rule to be fired repeatedly?";
	"notifytarget" LABEL "von der (Gruppe)"
		ENUM "primären Arbeitsgruppe","Eskalationsgruppe"
		DESC = "Which workgroup should be notified"
		REQUIRED;
	"targettable" LABEL "Bezugstabelle"
		ENUM "Meldung","Auftrag","keine"
		DESC = "Table that this rule applies to"
		REQUIRED;
	"categoryescalation" LABEL "categorycallesc"
		INTEGER;
	"escgroupescalation" LABEL "escalationgroup"
		INTEGER REQUIRED;
	"basetime" LABEL "basetime"
		ENUM "Gemeldet","Planzeit"
		DESC = "Base time for the escalation"
		DEFAULT = "Gemeldet" REQUIRED;
	"past" LABEL "past"
		INTEGER MAX "1" MIN "0"
		DESC = "If schedule for the past"
		DEFAULT = 1;
};

