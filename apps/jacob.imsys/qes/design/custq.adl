DEFINE TABLE accountingcode WITH DATASOURCE custqDataSource
	DESC "Kostenstellen KST"
	ORDER BY "code"
	PRIMARY KEY PKey("code")
	FOREIGN KEY contract_FKey("contract_key")
	FOREIGN KEY parentaccounting_FKey("parent_key")
	INFOFIELD "code"
{
	"code" LABEL "Kostenstelle"
		VSTRING OF LENGTH 8
		DESC = "Kostenstellenbezeichnung-PrimaryKey"
		REQUIRED;
	"contract_key" LABEL "contract_key"
		INTEGER;
	"accountingstatus" LABEL "Status"
		ENUM "ungültig","gültig"
		DESC = "Gültige Kostenstelle"
		DEFAULT = "gültig";
	"center" LABEL "Center"
		VSTRING OF LENGTH 30
		DESC = "Center  der Kostenstelle";
	"department" LABEL "Department"
		VSTRING OF LENGTH 20
		DESC = "Abteilung der Kostenstelle";
	"parent_key" LABEL "parent_key"
		VSTRING OF LENGTH 10
		DESC = "Link zur übergeordneten Kostengruppe";
};

DEFINE TABLE accountinggroup WITH DATASOURCE custqDataSource
	DESC "Kostenstellengruppe"
	PRIMARY KEY PKey("pkey")
	FOREIGN KEY paccountinggroup_FKey("parent_key")
	INFOFIELD "pkey"
{
	"pkey" LABEL "Pkey"
		VSTRING OF LENGTH 10
		DESC = "Primary Key"
		REQUIRED;
	"name" LABEL "Name"
		VSTRING OF LENGTH 20;
	"description" LABEL "Description"
		VSTRING OF LENGTH 255;
	"site" LABEL "Site"
		VSTRING OF LENGTH 32
		DESC = "Key von Faplis Site";
	"parent_key" LABEL "parent_key"
		VSTRING OF LENGTH 10;
};

