DEFINE TABLE qw_events
	DESC "Contains information about Events"
	ORDER BY "qwkey"
	SYSTEMTABLE
	PRIMARY KEY SYSTEMTABLEKEY PKey("qwkey")
	INDEX KEY SYSTEMTABLEKEY IKey1("action","state")
	INDEX KEY SYSTEMTABLEKEY IKey2("tablename","tablekey")
{
	"qwkey" LABEL "qwkey" READONLY
		SYSTEMFIELD
		SERIAL CACHE SIZE 5
		DESC = "Primary key";
	"when" "qw_when" LABEL "when"
		SYSTEMFIELD
		DATETIME
		DESC = "The date and time when the event should fire";
	"action" LABEL "action"
		SYSTEMFIELD
		INTEGER OF LENGTH 2
		DESC = "The action to be performed for the event. 0=OWNERS, 1=ALERT, 2=EMAIL, 3=FAX, 4=PAGER, 5=PRINTER, 6=COMMAND"
		DEFAULT = 0;
	"workgroupkey" LABEL "workgroupkey"
		SYSTEMFIELD
		INTEGER
		DESC = "The primary key of the workgroup table record if this event is aimed at a workgroup";
	"ownerkey" LABEL "ownerkey"
		SYSTEMFIELD
		INTEGER
		DESC = "The primary key of the owner table record if this event is aimed at a owner";
	"escalationkey" LABEL "escalationkey"
		SYSTEMFIELD
		INTEGER
		DESC = "If this is an escalation, the primary key of the escalation record which caused this event";
	"state" LABEL "state"
		SYSTEMFIELD
		INTEGER OF LENGTH 2
		DESC = "The state of the event for the QAlert process. 0=New, 1=Future, 2=Scheduled"
		DEFAULT = 0;
	"tablename" LABEL "tablename"
		SYSTEMFIELD
		VSTRING OF LENGTH 20
		DESC = "The name of the table that caused the escalation";
	"tablekey" LABEL "tablekey"
		SYSTEMFIELD
		INTEGER
		DESC = "The primary key of the table row that caused the escalation";
	"severity" LABEL "severity"
		SYSTEMFIELD
		INTEGER OF LENGTH 2
		DESC = "The severity of the message";
	"message" LABEL "message"
		SYSTEMFIELD
		VSTRING OF LENGTH 240
		DESC = "The message string. For email this is the subject";
	"addressee" LABEL "addressee"
		SYSTEMFIELD
		VSTRING OF LENGTH 240
		DESC = "The recipient address";
	"sender" LABEL "sender"
		SYSTEMFIELD
		VSTRING OF LENGTH 240
		DESC = "The sender of the message";
	"address2" LABEL "address2"
		SYSTEMFIELD
		VSTRING OF LENGTH 240
		DESC = "Secondary recipient address. For email this is the CC address";
	"escstatus" LABEL "escstatus"
		SYSTEMFIELD
		INTEGER
		DESC = "Used to determine whether an escalation event is still valid";
	"tier" LABEL "tier"
		SYSTEMFIELD
		INTEGER OF LENGTH 3
		DESC = "The tier of the groupmember record. Is ignored if -1";
	"repeatinterval" LABEL "repeatinterval"
		SYSTEMFIELD
		INTEGER OF LENGTH 6
		DESC = "Repeat interval in minutes";
	"agent" LABEL "agent"
		SYSTEMFIELD
		INTEGER OF LENGTH 2
		DESC = "The event server agent identifier. If 0 all rows are processed, if non 0 then only rows with this value are accesses"
		DEFAULT = 0;
	"datemodified" LABEL "datemodified"
		SYSTEMFIELD
		DATETIME
		DESC = "The date and time when the record was last modified";
	"rptschedkey" LABEL "rptschedkey"
		SYSTEMFIELD
		INTEGER
		DESC = "For a scheduled report, this is the key in qw_reports of the report's template";
	"workgroupkeyname" LABEL "workgroupkeyname"
		SYSTEMFIELD
		VSTRING OF LENGTH 40
		DESC = "The key name of the FKey to workgroup from the table listed in the	tablename field, if valued, it is used by event server to get the currently linked workgroup";
	"ownerkeyname" LABEL "ownerkeyname"
		SYSTEMFIELD
		VSTRING OF LENGTH 40
		DESC = "The key name of the FKey to the owner from the table listed in the	tablename field, if valued, it is used by event server to get the currently linked workgroup";
};

