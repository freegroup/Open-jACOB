	callevent;
DEFINE TABLESET callroles WITH TABLES
	callinfo,
	workgroup ALIAS callworkgroup
		DESC = "Auftragskoordinator"
		CONDITION "callworkgroup.wrkgrptype=0 OR callworkgroup.wrkgrptype=3 ",
	employee ALIAS agent
		CONDITION "agent.supportrole = 1",
